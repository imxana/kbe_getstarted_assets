 <root>
	<Properties>
	</Properties>

	<BaseMethods>
		<createCell>
			<!--Scene的CELL ENTITYCALL-->
			<Arg>   ENTITYCALL      </Arg>
		</createCell>
		<!--暴露给客户端的进入游戏的方法-->
		<enterGame>
			<!--暴露给客户端的方法，需要声明该属性-->
			<Exposed/>
		</enterGame>
	</BaseMethods>

	<ClientMethods>
		<!--进入游戏失败的回调-->
		<onEnterGameFailed>
			<!--错误代码-->
			<Arg>       INT8        </Arg>
		</onEnterGameFailed>
		<!--进入游戏成功的回调-->
		<onEnterGameSuccess>
			<!--无参-->
		</onEnterGameSuccess>
		<!--FirstEntity hello的回调-->
		<onFirstEntityHello>
			<Arg>           UNICODE         </Arg>
		</onFirstEntityHello>
	</ClientMethods>
</root>
